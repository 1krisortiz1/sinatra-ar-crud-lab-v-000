<!-- That view should use ERB to iterate over @articles and
render them on the page. -->
<h1>Articles</h1>

<ol>
    <% @articles.each do |article| %>
        <li>
            <a href="/posts/<%= post.id %>">
                <%= article.title %>
            </a>
            <p><% article.content %></p>
        </li>
    <% end %>
</ol>
